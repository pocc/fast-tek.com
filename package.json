{
  "name": "fast-tek",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler pages dev --compatibility-date=2024-12-01 -- npx vite",
    "build": "rm -rf dist && vite build && mv dist/demo/_redirects dist/_redirects && cp dist/demo/index.html dist/404.html",
    "preview": "wrangler pages dev dist",
    "deploy": "npm run build && wrangler pages deploy dist",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply fast-tek-db --local",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint src/ functions/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.36.0",
    "hono": "^4.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@eslint/js": "^9.16.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.28.0",
    "eslint": "^9.16.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^5.4.11",
    "vitest": "^2.1.8",
    "wrangler": "^3.93.0"
  }
}
